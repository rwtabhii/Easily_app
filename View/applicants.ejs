

        <% if (applicants.length > 0) { %>
        <table class="table table-striped table-bordered">
            <thead class="table-dark">
                <tr>
                    <th>No</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Conact</th>
                    <th>Resume</th>
                </tr>
            </thead>
            <tbody>
                <% applicants.forEach(applicant => { %>
                <tr>
                    <td><%= applicant.id %></td>
                    <td><%= applicant.name %></td>
                    <td><%= applicant.email %></td>
                    <td><%= applicant.contact %></td>
                    <td> <a href="/resume/<%=applicant.image%>" target="_blank" class = "modal-fullscreen">View</a>
                    </td>
                </tr>
                <% }) %>
            </tbody>
        </table>
        <% } else { %>
        <p class="text-center text-danger">No applicants found.</p>
        <% } %>
    </div>